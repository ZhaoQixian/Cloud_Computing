{
  "name": "sg-hangout-guide",
  "version": "1.0.0",
  "description": "A conversational widget for finding hangout spots in Singapore",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "keywords": [
    "singapore",
    "hangout",
    "guide",
    "conversational",
    "widget",
    "onemap",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9",
    "openai": "^4.95.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22"
  }
}
